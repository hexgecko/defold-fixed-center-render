local SPEED = 500

function init(self)
	msg.post("input#click", "subscribe_click")
end

function final(self)
	msg.post("input#click", "unsubscribe_click")
end

function on_message(self, message_id, message)
	if message_id == hash("click") then
		go.set_position(vmath.vector3(message.x, message.y, 0))
	end
end
